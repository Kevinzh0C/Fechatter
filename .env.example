# Fechatter Environment Configuration Example
# Copy this file to .env and update with your values

# === Database Configuration ===
DATABASE_URL=postgres://postgres:password@localhost:5432/fechatter
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=fechatter

# === Redis Configuration ===
REDIS_URL=redis://localhost:6379

# === API Configuration ===
API_HOST=0.0.0.0
API_PORT=6688

# === JWT Configuration ===
JWT_SECRET=your-secret-key-here-change-in-production
JWT_EXPIRES_IN=7d

# === OpenAI Configuration (for Bot Server) ===
OPENAI_API_KEY=your-openai-api-key-here

# === S3 Configuration (for File Storage) ===
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=fechatter-files

# === Service URLs ===
GATEWAY_URL=http://localhost:8080
FECHATTER_SERVER_URL=http://localhost:6688
NOTIFY_SERVER_URL=http://localhost:6687
BOT_SERVER_URL=http://localhost:6686
ANALYTICS_SERVER_URL=http://localhost:6690

# === NATS Configuration ===
NATS_URL=nats://localhost:4222

# === Meilisearch Configuration ===
MEILI_URL=http://localhost:7700
MEILI_MASTER_KEY=your-master-key-here

# === ClickHouse Configuration ===
CLICKHOUSE_URL=http://localhost:8123
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# === Frontend Configuration ===
VITE_API_BASE_URL=http://localhost:8080/api
VITE_WS_URL=ws://localhost:8080

# === Environment ===
NODE_ENV=development
RUST_LOG=info
